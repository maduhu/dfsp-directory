{
    "implementation": "dfsp-directory",
    "console": {
        "server": {
            "host": "0.0.0.0",
            "port": 30001
        }
    },
    "stdOut": {
        "mode": "dev"
    },
    "log": {
        "streams": [{
            "level": "trace",
            "stream": "../logRotateStream",
            "streamConfig": {
                "path": "ut5_%Y-%m-%d.log",
                "symlink": "ut5.log",
                "compress": true
            }
        }]
    },
    "masterBus": {
        "logLevel": "debug",
        "socket": "dfsp-directory"
    },
    "workerBus": {
        "logLevel": "debug"
    }
}
